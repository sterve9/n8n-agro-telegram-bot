    {
      "parameters": {
        "jsCode": "const TZ = \"Africa/Tunis\";\n\n// Recalcule \"hier\" (m√™me m√©thode)\nconst todayStr = new Intl.DateTimeFormat('en-CA', {\n  timeZone: TZ, year: 'numeric', month: '2-digit', day: '2-digit'\n}).format(new Date());\nconst pivotToday = new Date(`${todayStr}T00:00:00Z`);\nconst pivotYesterday = new Date(pivotToday.getTime() - 24 * 60 * 60 * 1000);\nconst ymd = new Intl.DateTimeFormat('en-CA', {\n  timeZone: TZ, year: 'numeric', month: '2-digit', day: '2-digit'\n}).format(pivotYesterday);\n\n// chatId s√ªr + d√©tection \"vide\"\nconst chatId = (items[0]?.json?.chatId) ?? $item(0).$node[\"Normalize Input\"].json.chatId ?? 0;\nconst hasItems = items.length > 0 && !items[0].json.__empty;\n\nif (!hasItems) {\n  const message = `üìä *Rapport d‚Äôexploitation ‚Äî ${ymd}*\\n\\nAucune donn√©e enregistr√©e hier.`;\n  return [{ json: { chatId, message } }];\n}\n\n// Grouper et formater\nconst fermeMap = {};\nfor (const { json } of items) {\n  const ferme = json.ferme || \"Ferme inconnue\";\n  const vanne = json.vanne || \"N/A\";\n  const intervention = json.intervention;\n  const pulverisation = json[\"pulv√©risation\"];\n  const produit = json.produit || \"inconnu\";\n  if (!fermeMap[ferme]) fermeMap[ferme] = [];\n  if (intervention) fermeMap[ferme].push(`   ‚û§ ${intervention} (Vanne ${vanne})`);\n  if (pulverisation) {\n    fermeMap[ferme].push(`   ‚û§ ${pulverisation}\\n     - Produit : ${produit}\\n     - Vanne : ${vanne}`);\n  }\n}\n\nlet message = `üìä *Rapport d‚Äôexploitation ‚Äî ${ymd}*\\n\\n`;\nfor (const [ferme, lignes] of Object.entries(fermeMap)) {\n  message += `üè° *${ferme}*\\n${lignes.join(\"\\n\")}\\n\\n`;\n}\nmessage = message.trim();\n\nreturn [{ json: { chatId, message } }];\n"
      },
